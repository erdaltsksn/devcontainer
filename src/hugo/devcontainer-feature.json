{
    "id": "hugo",
    "version": "1.0.0",
    "name": "Hugo",
    "documentationURL": "https://github.com/erdaltsksn/devcontainer/tree/main/src/hugo",
    "description": "Installs Hugo (A Fast and Flexible Static Site Generator)",
    "options": {
        "variant": {
            "type": "string",
            "default": "hugo_extended",
            "description": "Pick hugo variant",
            "proposals": [
                "hugo_extended",
                "hugo"
            ]
        },
        "version": {
            "type": "string",
            "default": "latest",
            "description": "Select or enter a version",
            "proposals": [
                "latest"
            ]
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "budparr.language-hugo-vscode",
                "eliostruyf.vscode-front-matter"
            ],
            "settings": {}
        }
    }
}
